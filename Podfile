xcodeproj 'daddys40.xcodeproj'
platform :ios, '7.0'

inhibit_all_warnings!

target "daddys40" do
    pod 'AFNetworking', git: 'https://github.com/balmbees/AFNetworking.git'
    pod 'Facebook-iOS-SDK', '3.7.1'
    pod 'TTTAttributedLabel', git: 'https://github.com/balmbees/TTTAttributedLabel.git'
    pod 'HPGrowingTextView', git: 'https://github.com/balmbees/GrowingTextView.git'
    pod 'MBProgressHUD', git: 'https://github.com/balmbees/MBProgressHUD.git'
    pod 'SSKeychain', '1.0.4'
    pod 'EKKeyboardAvoiding', '1.3'
    pod 'GoogleAnalytics-iOS-SDK', '3.0.3'
    pod 'UIActionSheet+Blocks'
end

target "daddys40Tests" do
    pod 'Kiwi/XCTest', git: 'https://github.com/breath103/Kiwi.git'
    pod 'OCMock', '2.2.1'
end

